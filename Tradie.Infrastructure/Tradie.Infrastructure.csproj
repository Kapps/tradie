<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>netcoreapp3.1</TargetFramework>
    <Platforms>AnyCPU;x64</Platforms>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="HashiCorp.Cdktf" Version="0.7.0" />
    <PackageReference Include="HashiCorp.Cdktf.Providers.Aws" Version="2.0.13" />
    <PackageReference Include="HashiCorp.Cdktf.Providers.Docker" Version="0.3.94" />
    <PackageReference Include="HashiCorp.Cdktf.Providers.Local" Version="0.0.89" />
    <PackageReference Include="HashiCorp.Cdktf.Providers.Null" Version="0.2.97" />
    <PackageReference Include="HashiCorp.Cdktf.Providers.Random" Version="0.0.5" />
  </ItemGroup>

  <ItemGroup>
    <None Remove="HashiCorp.Cdktf.Providers.Aws" />
    <None Remove="HashiCorp.Cdktf.Providers.Local" />
    <None Remove="Resources\" />
    <None Remove="cdktf.out\**" />
  </ItemGroup>
  <ItemGroup>
    <Folder Include="Resources\" />
  </ItemGroup>
  <ItemGroup>
    <Compile Remove="cdktf.out\**" />
  </ItemGroup>
  <ItemGroup>
    <EmbeddedResource Remove="cdktf.out\**" />
  </ItemGroup>
</Project>