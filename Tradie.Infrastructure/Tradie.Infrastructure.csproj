<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>net6.0</TargetFramework>
    <Platforms>AnyCPU;x64</Platforms>
    <Nullable>disable</Nullable>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="AWSSDK.ECR" Version="3.7.4.29" />
    <PackageReference Include="AWSSDK.SimpleSystemsManagement" Version="3.7.17.6" />
    <PackageReference Include="Docker.DotNet" Version="3.125.5" />
    <PackageReference Include="HashiCorp.Cdktf" Version="0.10.4" />
    <PackageReference Include="HashiCorp.Cdktf.Providers.Aws" Version="7.0.65" />
    <PackageReference Include="HashiCorp.Cdktf.Providers.Local" Version="0.4.62" />
    <PackageReference Include="HashiCorp.Cdktf.Providers.Random" Version="0.2.62" />
    <PackageReference Include="SharpCompress" Version="0.31.0" />
  </ItemGroup>

  <ItemGroup>
    <None Remove="HashiCorp.Cdktf.Providers.Aws" />
    <None Remove="HashiCorp.Cdktf.Providers.Local" />
    <None Remove="Resources\" />
    <None Remove="cdktf.out\**" />
  </ItemGroup>
  <ItemGroup>
    <Folder Include="Resources\" />
  </ItemGroup>
  <ItemGroup>
    <Compile Remove="cdktf.out\**" />
  </ItemGroup>
  <ItemGroup>
    <EmbeddedResource Remove="cdktf.out\**" />
  </ItemGroup>
